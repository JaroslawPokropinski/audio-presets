{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "nodemon",
    "build": "npm run build:next && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build:exe": "nexe ./dist/server.js -t windows-x64-12.16.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.0",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "fontsource-roboto": "^3.1.5",
    "next": "^10.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.30.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "nexe": "^3.3.7",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}